import tank/state

import entity/health
import entity/money

import square

byte selected_tank

const array(Tank) tanks = [export_square_tank]

void init_tank() {
  init_money()
  ammonia_level = $20
  oxygen_level = $80
  co2_level = $10
  selected_tank = 0
  active_tank = export_square_tank
}

void draw_tank() {
  call(active_tank.draw_func)
}